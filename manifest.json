{
  "name": "Serial Debugger",
  "version": "0.1.0",
  "manifest_version": 2,
  "offline_enabled": true,
  "description": "Debug serial commands",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "serial",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
